class OnboardingSettingsAdapter extends TypeAdapter<OnboardingSettings> {
  @override
  final int typeId = 9;

  @override
  OnboardingSettings read(BinaryReader reader) {
    final numOfFields = reader.readByte();
    final fields = <int, dynamic>{
      for (int i = 0; i < numOfFields; i++) reader.readByte(): reader.read(),
    };
    return OnboardingSettings(
      coachName: fields[0] as String,
      seasonName: fields[1] as String,
      teamName: fields[2] as String,
      isCompleted: fields[3] as bool,
    );
  }

  @override
  void write(BinaryWriter writer, OnboardingSettings obj) {
    writer
      ..writeByte(4)
      ..writeByte(0)
      ..write(obj.coachName)
      ..writeByte(1)
      ..write(obj.seasonName)
      ..writeByte(2)
      ..write(obj.teamName)
      ..writeByte(3)
      ..write(obj.isCompleted);
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is OnboardingSettingsAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}
